@article{corson_nirs_1999,
	title = {{NIRS}: {Forage} analysis and livestock feeding},
	issn = {1179-4577, 0369-3902},
	shorttitle = {{NIRS}},
	url = {http://www.nzgajournal.org.nz/index.php/ProNZGA/article/view/2340},
	doi = {10.33584/jnzg.1999.61.2340},
	abstract = {This paper describes analysis by near infrared reflectance spectroscopy (NIRS) and demonstrates the capacity of this technology to provide rapid, low cost and accurate estimates of feed composition. The need for an extensive database against which the instruments is calibrated and the importance of good wet chemistry backup to NIRS are emphasised, together with some of the diverse uses for NIRS in research and agriculture. Examples are given of contrasting analyses for quality of maize silage and pastures. NIRS prediction of pasture, silage and supplement composition can be used in conjunction with a ration balancing model developed for the New Zealand dairy industry, allowing farmers to formulate diets able to meet desired levels of production.},
	language = {en},
	urldate = {2022-01-29},
	journal = {Proceedings of the New Zealand Grassland Association},
	author = {Corson, D.C. and Waghorn, G.C. and Ulyatt, M.J. and Lee, J.},
	month = jan,
	year = {1999},
	pages = {127--132},
	file = {Corson et al. - 1999 - NIRS Forage analysis and livestock feeding.pdf:/Users/ira/Zotero/storage/RHLCKYWW/Corson et al. - 1999 - NIRS Forage analysis and livestock feeding.pdf:application/pdf},
}

