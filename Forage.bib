
@article{laflamme_effects_1989,
	title = {{EFFECTS} {OF} {STORAGE} {CONDITIONS} {FOR} {LARGE} {ROUND} {BALES} {ON} {QUALITY} {OF} {GRASS}-{LEGUME} {HAY}},
	volume = {69},
	issn = {0008-3984, 1918-1825},
	url = {http://www.nrcresearchpress.com/doi/10.4141/cjas89-110},
	doi = {10.4141/cjas89-110},
	abstract = {Two experirnents were conducted to assess the effects of storage conditions on the quality of large round bales (LRB). Samples were taken at 75-d intervals from LRB stored inside, outside single bales covered with a l5-mm black polyethylene plastic bag, outside uncovered as single bales, outside uncovered in a single row and outside uncovered in a double stacked row. Bales were sampled at depths of 0-15, 15-30 and 30-45 cm. Time, storage and depth factors had an influence on the chemical components of the LRB except for protein. Wearhering had the most effect after 150 d of storage and with LRB stored uncovered and in the top 15-cm layer. Significant (P{\textless}0.05) interactions in in vitro dry matter digestibilities were found for storage time x depth of sample, storage time x storage methods and for depth of sample x storage methods. Under the conditions of our experiments, weathering damages were limited to the superficial layer of LRB left uncovered.},
	language = {en},
	number = {4},
	urldate = {2022-01-29},
	journal = {Canadian Journal of Animal Science},
	author = {Laflamme, L. F.},
	month = dec,
	year = {1989},
	pages = {955--961},
	file = {Laflamme - 1989 - EFFECTS OF STORAGE CONDITIONS FOR LARGE ROUND BALE.pdf:/Users/ira/Zotero/storage/T8KAMAEW/Laflamme - 1989 - EFFECTS OF STORAGE CONDITIONS FOR LARGE ROUND BALE.pdf:application/pdf},
}

